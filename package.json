{
  "name": "sunrise-sunset.org",
  "version": "1.1.0",
  "description": "Library for sunrise-sunset.org's API",
  "main": "./dir/index.js",
  "scripts": {
    "ts-watch": "npm run ts-watch-main && npm run ts-watch-wiki",
    "ts-watch-main": "tsc ./tsconfig.json --watch",
    "ts-watch-wiki": "tsc ./wiki/tsconfig.json --watch",
    "scss-watch": "sass --watch wiki/styles/src:wiki/styles/dir",
    "sass-watch": "npm run scss-watch"
